  <div class="row">
    <div class="col s12 m7">
      {{#each posts as |blogpost|}}
      {{log blogpost}}
      <div class="card">
        <div class="card-image">
          <img src="images/sample-1.jpg">
          <span class="card-title">{{blogpost.post_title}}</span>
        </div>
        <div class="card-content">
          <p>By {{blogpost.user.username}} on {{blogpost.created_at}}</p>
          <p>{{blogpost.post_content}}</p>
        </div>
        <div class="card-action">
          <a href="/post/{{blogpost.id}}">READ MORE</a>
        </div>
      </div>
      {{/each}}
    </div>
  </div>
