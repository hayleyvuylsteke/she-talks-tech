  <div>Can I see this text?</div>
  
  <div class="row">
    <div class="col s12 m7">
      {{#each blogpost}}
      <div class="card">
        <div class="card-image">
          <img src="images/sample-1.jpg">
          <span class="card-title">{{post_title}}</span>
        </div>
        <div class="card-content">
          <p>By {{username}} on {{created_at}}</p>
          <p>{{post_content}}</p>
        </div>
        <div class="card-action">
          <a href="/post/{{blogpost.id}}">READ MORE</a>
        </div>
      </div>
      {{/each}}
    </div>
  </div>
